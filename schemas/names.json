{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "additionalProperties": false,
    "definitions": {
        "nameObject": {
            "additionalProperties": false,
            "type": "object",
            "properties": {
                "id":       { "type": "number" },
                "realName": { "type": "string" },
                "fullName": { "type": "string" },
                "alias":    { "type": "string" },
                "social":   { "type": "string" }
            },
            "required": ["id", "realName", "fullName", "alias", "social"]
        }
    },

    "type": "object",

    "properties": {
        "index": { "type": "number", "default": 1},
        "items": {
            "type": "array",
            "items": { "$ref": "#/definitions/nameObject" },
            "default": []
        }
    },
    "required": ["index"]
  }